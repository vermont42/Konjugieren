    "Info.creditsText" : {
      "localizations" : {
        "de" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "`Entwickler`\n~Konjugieren~ wurde von ~Josh Adams~ entwickelt, einem iOS-App-Entwickler mit Sitz in der NÃ¤he von San Francisco, Kalifornien. Er ist auch der Entwickler von Conjuguer, Conjugar und RaceRunner, die alle im iOS App Store erhÃ¤ltlich sind. Der Quellcode von Konjugieren ist hier verfÃ¼gbar: %https://github.com/vermont42/Konjugieren%"
          }
        },
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "`Creator`\n~Konjugieren~ was created by ~Josh Adams~, an iOS-app developer based near San Francisco, California. He is also the developer of Conjuguer, Conjugar, and RaceRunner, all available in the iOS App Store. Konjugieren's source code is available here: %https://github.com/vermont42/Konjugieren%"
          }
        }
      }
    },
