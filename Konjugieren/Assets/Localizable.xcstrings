    "Info.creditsText" : {
      "localizations" : {
        "de" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "`Entwickler`\n~Konjugieren~ wurde von ~Josh Adams~ entwickelt, einem iOS-App-Entwickler mit Sitz in der N√§he von San Francisco, Kalifornien. Er ist auch der Entwickler von Conjuguer, Conjugar und RaceRunner, die alle im iOS App Store erh√§ltlich sind. Der Quellcode von Konjugieren ist hier verf√ºgbar: %https://github.com/vermont42/Konjugieren%"
          }
